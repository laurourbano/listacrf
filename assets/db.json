        {
            "departamento": "CAPTURAR LIGAÇÕES",
            "instrucoes": [
                "Do mesmo grupo, digite 55;",
                "De outro grupo, digite 54 + ramal desejado."
            ]
        },
        {
            "departamento": "TELEFONE FIXO DE MESMO DDD",
            "instrucoes": [
                "Digite 0 + (Senha pessoal) + (aguardar o tom de linha) + número desejado",
                "Exemplo: 0*********3363-0000"
            ]
        },
        {
            "departamento": "CHAMADA EM ESPERA",
            "instrucoes": [
                "Colocar em espera: código flash + 6 + posição de espera (de 0 a 9);",
                "Recuperar chamada em espera: código 56 + posição de espera (de 0 a 9);"
            ]
        },
        {
            "departamento": "TELEFONE FIXO DE OUTRO DDD",
            "instrucoes": [
                "Digite 0 + (Senha pessoal) + (aguardar o tom de linha) + operadora (preferencialmente 015) + código de área + número desejado",
                "Exemplo: 0*********015443363-0000",
                "CASCAVEL=021 / LONDRINA 015 / PONTA GROSSA 014"
            ]
        },
        {
            "departamento": "IDENTIFIQUE CHAMADAS INTERNAS E EXTERNAS",
            "instrucoes": [
                "Ligações externas: 2 toques breves e próximos",
                "Ligações internas: toque longo e distinto"
            ]
        },
        {
            "departamento": "TELEFONE CELULAR DE MESMO DDD",
            "instrucoes": [
                "Digite 0 + (Senha pessoal) + (aguardar o tom de linha) + número desejado",
                "Exemplo: 0*********9999-0000"
            ]
        },
        {
            "departamento": "SCRIPT DE ATENDIMENTO",
            "exemplo": "Bom dia/tarde, Conselho Regional de Farmácia, SEU NOME"
        },
        {
            "departamento": "TELEFONE CELULAR DE OUTRO DDD",
            "instrucoes": [
                "Digite 0 + (Senha pessoal) + (aguardar o tom de linha) + operadora 041 + código de área + número desejado",
                "Exemplo: 0*********0414499999-0000"
            ]
        },
        {
            "departamento": "ANOTE OS RECADOS",
            "instrucoes": [
                "Caso o seu colega esteja ausente e o ramal tomar, capture a ligação, atenda e anote recado conforme exemplo abaixo:",
                "- Anote o nome, assunto e contato. Entregue a anotação."
            ]
        }
    ]
